    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ngx-glide.mjs",
      "es2020": "./fesm2020/ngx-glide.mjs",
      "es2015": "./fesm2015/ngx-glide.mjs",
      "node": "./fesm2015/ngx-glide.mjs",
