{
  "name": "ngx-glide",
  "version": "14.0.0",
  "description": "Angular wrapper component of Glide carousel library",
  "peerDependencies": {
    "@angular/common": "14.x",
    "@angular/core": "14.x",
    "@glidejs/glide": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avivharuzi/ngx-glide.git"
  },
  "keywords": [
    "angular",
    "library",
    "glide",
    "slider",
    "carousel"
  ],
  "author": "Aviv Haruzi <avivharuzi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avivharuzi/ngx-glide/issues"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "homepage": "https://github.com/avivharuzi/ngx-glide#readme",
  "module": "fesm2015/ngx-glide.mjs",
  "es2020": "fesm2020/ngx-glide.mjs",
  "esm2020": "esm2020/ngx-glide.mjs",
  "fesm2020": "fesm2020/ngx-glide.mjs",
  "fesm2015": "fesm2015/ngx-glide.mjs",
